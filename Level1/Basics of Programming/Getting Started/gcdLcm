import java.util.*;
    
    public class Main{
    
    public static void main(String[] args) {
      // write your code here  
      Scanner sc = new Scanner(System.in);
      int n1 = sc.nextInt();
      int n2 = sc.nextInt();
      int temp1=n1;
      int temp2 = n2;
      while(n1%n2!=0){
        int rem = n1%n2;
        n1=n2;
        n2=rem;
      }
      System.out.println(n2);
      System.out.println((temp1*temp2)/n2);
     }
    }
